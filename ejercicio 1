// Solicita dos números entre 1 y 50
let num1 = parseInt(prompt("Ingresar primer número (1-50):"));
let num2 = parseInt(prompt("Ingresar segundo número (1-50):"));

// Verifica que estén en el rango
if ((num1 >= 1 && num1 <= 50) && (num2 >= 1 && num2 <= 50)) {
  for (let i = 1; i <= 50; i++) {
    if (i === num1 || i === num2) {
      console.log("¡Lotería!");
    } else {
      console.log(i);
    }
  }
} else {
  console.log("Error: Los números deben estar entre 1 y 50.");
}
